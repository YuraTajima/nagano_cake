

<main>
    <h2>商品一覧</h2>
    <div>
        <table class="table table-hover table-inverse">
        <thead>
            <tr>
                <th>商品ID</th>
                <th>商品名</th>
                <th>税抜価格</th>
                <th>ジャンル</th>
                <th>ステータス</th>
            </tr>
        </thead>
        
        <tobody>
            <% @items.each do |item| %>
            <tr>
                <th><%= item.id %></th>
                <th><%= item.name %></th>
                <th><%= item.price %></th>
                <th><%= item.genre_id %></th>
                <th><%= item.is_active %></th>
            </tr>
            <% end %>
        </tobody>
        </table>
    </div>
</main>
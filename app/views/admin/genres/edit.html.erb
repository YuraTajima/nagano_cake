<main class="mx-5 my-3">
    <div class="mx-5">
        <h3>ジャンル編集</h3>
    </div>
    
    <div>
        <%= form_with model: @genre, url: admin_genre_path do |f| %>
        <div class="row mt-3">
        <p class="col-1">ジャンル名</p>
        <p class="col-2"><%= f.text_field :name %>
        <p class="col-1"><%= f.submit '変更を保存' %></p>
        </div>
        <% end %>
    </div>
</main>
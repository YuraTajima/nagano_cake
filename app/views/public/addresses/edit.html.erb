<main>
    <div>
        <h3>配送先編集</h3>
    </div>
    
    <div>
        <%= form_with model: @address, url: address_path, method: :patch do |f| %>
        
        <span>郵便番号(ハイフンなし)</span>
         <%= f.text_field :postal_code %>
         
         <span>住所</span>
          <%= f.text_field :address, size: 40  %>
          
        <%= f.text_field :name %>
        
        <%= f.submit '変更内容を保存' %>
        <% end %>
    </div>
</main>